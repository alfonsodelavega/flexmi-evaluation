				<EStringToStringMapEntry key="documentation" value="A floating point number in IEEE format"/>
			</EAnnotation>
			<attr name="nBits" lowerBound="1" defaultValueLiteral="32" type="//EInt">
				<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
					<EStringToStringMapEntry key="documentation" value="The number of bits. Float = 32, Double = 64"/>
				</EAnnotation>
			</attr>
			<attr name="endianess" lowerBound="1" defaultValueLiteral="BIG" type="//EndianessE">
				<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
					<EStringToStringMapEntry key="documentation" value="The endianess of the included data"/>
				</EAnnotation>
			</attr>
		</class>
		<class name="Byte">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="8 Bits of unknown content"/>
			</EAnnotation>
		</class>
		<class name="Character">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="A single character which can be of different encodings. The default size is 8 Bit. "/>
			</EAnnotation>
			<attr name="encoding" lowerBound="1" type="//EString">
				<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
					<EStringToStringMapEntry key="documentation" value="Character encoding table used for the content. E.g. ASCII or UNICODE. If the encoding has more than one byte per character. nBits must be set accordingly."/>
				</EAnnotation>
			</attr>
			<attr name="nBits" lowerBound="1" defaultValueLiteral="8" type="//EInt">
				<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
					<EStringToStringMapEntry key="documentation" value="The number of character encoding bits. The default is 8."/>
				</EAnnotation>
			</attr>
		</class>
		<EEnum name="EndianessE">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="Possible options for the endianess of data types"/>
			</EAnnotation>
			<EEnumLiteral name="BIG">
				<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
					<EStringToStringMapEntry key="documentation" value="Big endian: The most significant bit comes first"/>
				</EAnnotation>
			</EEnumLiteral>
			<EEnumLiteral name="LITTLE" value="1">
				<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
					<EStringToStringMapEntry key="documentation" value="Little endian: The least significant bit comes first."/>
				</EAnnotation>
			</EEnumLiteral>
		</EEnum>
		<class name="Boolean">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="A Boolean value, i.e. true or false."/>
			</EAnnotation>
		</class>
