	<class name="DialogPanel">
		<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
			<EStringToStringMapEntry key="documentation" value="Pagina utilizzabile per messaggi di feedback/conferme etc."/>
		</EAnnotation>
		<EAnnotation source="uipaneldoc">
			<EStringToStringMapEntry key="description" value="<p>
				Il <b>DialogPanel</b> &egrave; un pannello utilizzato per realizzare funzioni di feedback utente
				(messaggi informativi, dichieste di conferme). E'associato all'intera schermata e, quando &egrave; 
				attivato, mostra all'utente i messaggi previsti sostituendo completamente la schermata operativa.
				<br/>
				All'interno del singolo tab &egrave; possibile inserire solo quelle tipologie di pannelli che servono per:
				<ul>
					<li>mostrare messaggi formattati all'utente (<b>MsgBoxPanel</b>)</li>
					<li>fornire all'utente i comandi per &quot;rispondere&quot; al feedback, ovvero uno o pi&ugrave;
						pulsanti di conferma o cancellazione (<b>CommandPanel</b>)</li>
				</ul>
			</p>"/>
		</EAnnotation>
		<EAnnotation source="logicpkg">
			<EStringToStringMapEntry key="pkg" value="ui.panels"/>
		</EAnnotation>
		<ref name="commands" containment="true" type="CommandPanel">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="I pulsanti associati al dialog."/>
			</EAnnotation>
		</ref>
		<ref name="msgBoxes" upperBound="-1" containment="true" type="MsgBoxPanel">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="I messaggi di feedback da visualizzare devono essere inseriti in uno o più
				<b>MessageBoxPanel</b>."/>
			</EAnnotation>
		</ref>
	</class>
