	<class name="DataWidget">
		<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
			<EStringToStringMapEntry key="documentation" value="Classe astratta dalla quale discendono tutte le classi che rappresentano
			widget associabili a dati (<b>ApplicationData</b>). Un <b>DataWidget</b>
			ha un concetto di <i>valore</i> del widget che rappresenta l'input inserito."/>
		</EAnnotation>
		<EAnnotation source="logicpkg">
			<EStringToStringMapEntry key="pkg" value="ui.widgets"/>
		</EAnnotation>
		<attr name="dataTypeModifier" type="//EString">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
