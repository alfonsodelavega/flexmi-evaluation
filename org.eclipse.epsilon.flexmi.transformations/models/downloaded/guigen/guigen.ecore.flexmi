		<attr name="value" type="//UDLRCSpecConstants">
			<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
				<EStringToStringMapEntry key="documentation" value="Indica in quale dei 5 quadranti (UP/DOWN/LEFT/RIGHT/CENTER) deve
				essere posizionato l'elemento"/>
			</EAnnotation>
		</attr>
	</class>
	<EEnum name="UDLRCSpecConstants">
		<EAnnotation source="http://www.eclipse.org/emf/2002/GenModel">
			<EStringToStringMapEntry key="documentation" value="Valori ammissibili per l' <b>UDLRCWidgetLayoutSpecifier</b>."/>
		</EAnnotation>
		<EAnnotation source="logicpkg">
			<EStringToStringMapEntry key="pkg" value="ui.layout"/>
		</EAnnotation>
		<EEnumLiteral name="UP"/>
		<EEnumLiteral name="DOWN" value="1"/>
		<EEnumLiteral name="LEFT" value="2"/>
		<EEnumLiteral name="RIGHT" value="3"/>
		<EEnumLiteral name="CENTER" value="4"/>
	</EEnum>
	<class name="Image">
