          <tags name="EStringToStringMapEntry">
            <attributes name="key" value="documentation"/>
            <attributes name="value" value="Specificatore del posizionamento di questo pannello all'intenro del layout&#xD;&#xA;del pannello contenitore."/>
          </tags>
        </tags>
      </tags>
      <tags name="attr">
