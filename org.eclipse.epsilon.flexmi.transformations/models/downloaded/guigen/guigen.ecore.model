      <tags name="attr">
        <attributes name="name" value="visibleLines"/>
        <attributes name="type" value="//EInt"/>
        <tags name="EAnnotation">
          <attributes name="source" value="http://www.eclipse.org/emf/2002/GenModel"/>
          <tags name="EStringToStringMapEntry">
            <attributes name="key" value="documentation"/>
            <attributes name="value" value="numero di linee visibili senza bisogno di utilizzare la scrollbar in caso di &#xD;&#xA;&lt;b>ComboBox&lt;/b> a selezione &lt;i>multipla&lt;/i>. Se impostato a zero il numero di &#xD;&#xA;linee visualizzabili non &amp;egrave; predicibile. Nel caso di selezione singola invece&#xD;&#xA;l'attributo non ha effetto."/>
          </tags>
        </tags>
