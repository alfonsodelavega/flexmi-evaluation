    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Classe astratta dalla quale discendono tutti i vari tipi di widget."/>
    </eAnnotations>
    <eAnnotations source="logicpkg">
      <details key="pkg" value="ui.widgets"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Codice identificativo del widget. Deve essere univoco all'interno del &#xD;&#xA;&lt;b>ContentPanel&lt;/b> che lo contiene e deve essere un identificativo&#xD;&#xA;valido java."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="label" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Etichetta associata al widget."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="layoutSpec" eType="#//WidgetLayoutSpecifier"
        containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Specifica di posizionamento del widget all'interno del pannello che lo contiene.&#xD;&#xA;Il tipo dell'oggetto associato deve essere coerente con il tipo di layout del&#xD;&#xA;pannello contenitore."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="eventHandlers" upperBound="-1"
        eType="#//EventHandler" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Gestori degli eventi associati al widget. Le tipologie di evento gestite sono&#xD;&#xA;dipendenti dal particolare tipo di widget."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="securityConstraints" upperBound="-1"
        eType="#//UISecurityConstraint" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="E' possibile specificare uno o pi&amp;ugrave; security constraints sul widget, che&#xD;&#xA;permettono di rendere condizionata la visualizzazione o la abilitazione del&#xD;&#xA;widget a fronte delle condizioni descritte nel constraint.&#xD;&#xA;I constraint sono additivi (nel senso booleano) per ciascun &lt;i>constrained behavior&lt;/i>&#xD;&#xA;(comportamento condizionato = {visibilit&amp;agrave; / abilitazione}): se almeno un&#xD;&#xA;constraint &amp;egrave; verificato il comportamento condizionato sar&amp;agrave; abilitato&#xD;&#xA;(visibilit&amp;agrave; / editabilit&amp;agrave;).&#xD;&#xA;Se nessun constraint sar&amp;agrave; verificato, la visualizzazioen/abilitazione sarà condizionata solo allo stato&#xD;&#xA;di default / comandato, ovvero al valore delle proprietà &lt;b>defaultVisible&lt;/b> o &#xD;&#xA;&lt;b>defaultEnabled&lt;/b> se non è stato in precedenza utilizzato nessun&#xD;&#xA;comando di tipo &lt;b>ONOFFCommand&lt;/b> o &lt;b>VisibilityCommand&lt;/b>; se &#xD;&#xA;tali comandi sono stati utilizzati allora &quot;vince&quot; l'impostazione derivante da&#xD;&#xA;questi comandi.&#xD;&#xA;Se, infine, un constraint &amp;egrave; verificato, comunque un eventuale&#xD;&#xA;disabilitazione/invisibilit&amp;agrave; comandata (tramite &lt;b>ONOFFCommand&lt;/b>&#xD;&#xA; o &lt;b>VisibilityCommand&lt;/b>) vince."/>
      </eAnnotations>
