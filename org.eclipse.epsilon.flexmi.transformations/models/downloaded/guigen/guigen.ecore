  <eClassifiers xsi:type="ecore:EClass" name="ExecCommand" eSuperTypes="#//Command">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Permette di eseguire logica custom all'intenro di una catena di comandi.&#xD;&#xA;Al metodo specificato in &lt;i>methodName&lt;/i> sar&amp;agave; passata una struttura&#xD;&#xA;dati contenente:&#xD;&#xA;&lt;ul>&#xD;&#xA;&lt;li>i riferimenti agli &lt;b>ApplicationData&lt;/b> referenziati&#xD;&#xA;dal &lt;b>ContentPanel&lt;/b> che contiene il comando.&lt;/li>&#xD;&#xA;&lt;li>un riferimento alla sessione applicativa&lt;/li>&#xD;&#xA;&lt;li> alcune informazioni ausiliarie diepndenti dalla struttura del &#xD;&#xA;    &lt;b>ContentPanel&lt;/b> (es. Id del nodo selezionato in un tree, &#xD;&#xA;    informazioni circa il tab su cui si è cliccato, informazioni sulla cella selezionata&#xD;&#xA;in una tabella) &lt;/li>&#xD;&#xA;&lt;/ul>&#xD;&#xA;Nel metodo il programmatore potr&amp;agrave; eseguire qualsiasi tipo di logica ed in &#xD;&#xA;particolare:&#xD;&#xA;&lt;ul>&#xD;&#xA;&lt;li> modificare lo stato degli application data o della sessione.&lt;/li>&#xD;&#xA;&lt;li> inserire nel contesto applicativo messaggi informativi o di errore&lt;/li>&#xD;&#xA;&lt;li> determinare la prosecuzione del flusso, selezionando uno tra i &#xD;&#xA;&lt;i>results&lt;/i> disponibili.&lt;/li>&#xD;&#xA;&lt;/ul>&#xD;&#xA;Al termine dell'esecuzione del metodo il flusso proseguir&amp;agrave; con il comando&#xD;&#xA;inserito nel &lt;b>CommandOutcome&lt;/b> specificato al termine della logica.&#xD;&#xA;"/>
    </eAnnotations>
    <eAnnotations source="logicpkg">
      <details key="pkg" value="cmd"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="results" upperBound="-1"
        eType="#//CommandOutcome" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="L'elenco delle possibili prosecuzioni a fronte dell'esecuzione del comando."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="methodName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Nome del metodo nel quale sarà contenuta la logica effettiva.&#xD;&#xA;L'implementazione dipende dalla particolare cartuccia di generazione.&#xD;&#xA;Nel caso della cartuccia &quot;struts2&quot; verr&amp;agrave; generato un metodo nello&#xD;&#xA;strato spring, contenente una regione protetta nel quale il programamtore&#xD;&#xA;dovr&amp;agrave; inserire la logica effettiva."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="postExecData" upperBound="-1"
        eType="#//ApplicationData">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Occorre dichiarare preventivamente gli application data che saranno modificati nella logica applicativa."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="customTemplate" eType="#//CustomTemplate"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="extraModels" upperBound="-1"
        eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Eventuali modelli (porzioni di modello) aggiuntivi utilizzabili dalle varie cartucce&#xD;&#xA;di generazione 100% (es. modello del mapping RDB, ... modello di una&#xD;&#xA;interfaccia di servizio, ...)&#xD;&#xA;&#xD;&#xA;&lt;b>Attenzione&lt;/b>: Questa opzione &amp;egrave; da considerarsi una possibilit&amp;agrave; &lt;b>avanzata&lt;/b> &#xD;&#xA;da utilizzarsi con cautela."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="inlineCodeSnippets" upperBound="-1"
        eType="#//InlineCodeSnippet" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="&lt;p>Sono disponibili le seguenti chiavi:&lt;/p>&#xD;&#xA;&lt;table border=&quot;1&quot;>&lt;tbody>&#xD;&#xA;&lt;tr>&#xD;&#xA;&lt;th>&lt;b>Name&lt;/b>&lt;/th>&#xD;&#xA;&lt;th>&lt;b>Position&lt;/b>&lt;/th>&#xD;&#xA;&lt;th>&lt;b>Descrizione&lt;/b>&lt;/th>&#xD;&#xA;&lt;/tr>&#xD;&#xA;&lt;tr>&#xD;&#xA;&lt;td>body&lt;/td>&#xD;&#xA;&lt;td>executedMethod&lt;/td>&#xD;&#xA;&lt;td>si sostituisce alla regione protetta del body del metodo associato al comando&lt;/td>&#xD;&#xA;&lt;/tr>&#xD;&#xA;&lt;/tbody>&lt;/table>"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EventHandler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Gestore di un evento di User Interaction."/>
    </eAnnotations>
    <eAnnotations source="logicpkg">
      <details key="pkg" value="ui.event"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="command" eType="#//Command"
        containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A fronte dell'evento specificato in &lt;i>eventType&lt;/i> sar&amp;agrave; eseguito&#xD;&#xA;il comando specificato in questo elemento."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="eventType" eType="#//EventTypes">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Specifica il tipo di evento per cui &amp;egrave; definito questo event handler."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="skipValidation" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Se impostato a true non saranno eseguite le logiche automatiche di conversione/validazione&#xD;&#xA;a fronte dell'occorrenza dell'evento specificato.&#xD;&#xA;Serve per gestire casi in cui la non conformit&amp;agrave; dei dati immessi non&#xD;&#xA;&amp;egrave; importante, anzi deve essere ignorata (es. il tasto &quot;indietro&quot; o &quot;annulla&quot;)."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="methodProtection" eType="#//MethodProtectionTypes"
        defaultValueLiteral="REJECT_SAME">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Permette di proteggere l'esecuzione del metodo associato all'&lt;b>EventHandler&lt;/b>&#xD;&#xA;da effetti indesiderati derivanti dal'eventuale esecuzione contemporanea di altri metodi.&#xD;&#xA;Le possibili protezioni sono:&#xD;&#xA;&lt;ol>&#xD;&#xA;&lt;li>&lt;b>allowAll&lt;/b>: permette l'esecuzione contemporanea di qualsiasi metodo (nessuna &#xD;&#xA;protezione)&lt;/li>&#xD;&#xA;&lt;li>&lt;b>rejectSame&lt;/b> (&lt;i>default&lt;/i>): evita l'esecuzione contemporanea di altre istanze dello stesso &#xD;&#xA;metodo (protezione tipica del doppio click su uno stesso pulsante)&lt;/li>&#xD;&#xA;&lt;li>&lt;b>rejectAll&lt;/b>: evita l'esecuzione contemporanea di altre istanze di qualsiasi &#xD;&#xA;metodo (protezione cautelativa globale)&lt;/li>&#xD;&#xA;&lt;/ol>"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="eventSpecifiers" upperBound="-1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Permette di specificare ulteriormente la gestione dell'evento. &#xD;&#xA;E' possibile inserire 0, 1 o molti specificatori:&#xD;&#xA;&lt;ul>&#xD;&#xA;&lt;li>lo specificatore viene scritto in formato testo e la &#xD;&#xA; sintassi di ciascun specificatore &amp;egrave; tipicamente del tipo:&#xD;&#xA;&amp;lt;cod_specificatore&amp;gt;=&amp;lt;val_specificatore&amp;gt; &#xD;&#xA;(esempio: &lt;i>min_chars=3&lt;/i>)&#xD;&#xA;&lt;/li>&#xD;&#xA;&lt;li>le possibili valorizzazioni dipendono fortemente dal tipo di evento gestito.&#xD;&#xA;Attualmente le possibiit&amp;agrave; sono:&#xD;&#xA;  &lt;ul>&#xD;&#xA;  &lt;li>evento &lt;b>KEYPRESSED&lt;/b>:&#xD;&#xA;    &lt;ul>&#xD;&#xA;    &lt;li>&lt;i>min_chars=&amp;lt;numero&amp;gt;&lt;/i>: permette di specificare il numero&#xD;&#xA;           di caratteri minimo affinch&amp;egrave; scatti la query di &lt;i>suggestion&lt;/i>&#xD;&#xA;           (il default &amp;egrave; 4)&#xD;&#xA;     &lt;/li>&#xD;&#xA;    &lt;/ul>&#xD;&#xA;  &lt;/li>&#xD;&#xA;  &lt;/ul>&#xD;&#xA;&lt;/li>&#xD;&#xA;&lt;/ul>"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
